{
  "name": "openvtx-configurator",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:gh-pages": "env DEPLOYMENT=gh-pages vue-tsc --noEmit && vite build --outDir docs",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "pwa": "npm run build && npx http-server ./dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@headlessui/vue": "^1.6.2",
    "@heroicons/vue": "^1.0.6",
    "bulma": "^0.9.4",
    "isomorphic-fetch": "^3.0.0",
    "octokit": "^2.0.3",
    "pinia": "^2.0.13",
    "semver": "^7.3.7",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14",
    "web-serial-polyfill": "^1.0.13"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/dom-serial": "^1.0.2",
    "@types/node": "^16.11.27",
    "@types/semver": "^7.3.9",
    "@types/w3c-web-usb": "^1.0.6",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "http-server": "^14.1.1",
    "prettier": "^2.5.1",
    "sass": "^1.52.1",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.7"
  }
}
